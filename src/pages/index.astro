---
import Layout from '../layouts/Layout.astro';
import Countdown from '../sections/Header/Countdown.astro'
import '../styles/style.css'
import '@fontsource-variable/quicksand';
import Location from '../components/Location.astro';
import Header from '../components/Header.astro';
import { TimeLine } from '../sections/OrdenDia/TimeLine';
import Post from '../sections/Post/Post';
import { Registration } from '../components/Registration';
import Food from '../components/Food.astro';

---

<Layout title="Melissa & Pedro - Save the date 7, Dic">
	<main id='main' class=" h-screen ">
		<Header />
		<section class="flex flex-col gap-28">
			<TimeLine />
			<Location />
			<Post client:visible />
			<Food />
			<Registration client:visible />
		</section>
		</main>
</Layout>

